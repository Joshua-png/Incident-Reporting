{
  "name": "incident-reporting",
  "version": "1.0.0",
  "description": "API to aid in incident reports for insurance clients",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "migrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "test": "cross-env NODE_ENV=test mocha --timeout 10000 --exit",
    "test:setup": "npm run db:test && npm run migrate:test && npm test"
  },
  "mocha": {
    "timeout": 5000
  },
  "keywords": [
    "nodejs",
    "axios",
    "mocha",
    "chai",
    "express",
    "sequelize",
    "nodemon",
    "express-async-errors",
    "cors",
    "postgres"
  ],
  "author": "Joshua Aryee",
  "license": "ISC",
  "dependencies": {
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "mocha": "^10.4.0",
    "sequelize-cli": "^6.6.2"
  }
}
